<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:height="30dp" android:width="30dp" tools:ignore="StateListReachable">
        <layer-list>
            <item android:top="4dp" android:bottom="4dp" android:left="4dp" android:right="4dp"
                android:drawable="@drawable/switch_like" android:width="30dp" android:height="30dp"/>
        </layer-list>
    </item>

    <item android:height="30dp" android:width="30dp" tools:ignore="StateListReachable">
        <layer-list>
            <item android:top="4dp" android:bottom="4dp" android:left="4dp" android:right="4dp"
                android:drawable="@drawable/switch_rolling_eyes" android:width="30dp" android:height="30dp"/>
        </layer-list>
    </item>
</selector>